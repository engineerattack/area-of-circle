elif(a==3):
    def multiply(n1,n2):
        mul=0
        if n2<0:
            n2=-n2
            n1=-n1
        for i in range(1,n2+1):
            mul = mul+n1
        return mul
    num1= int(input("Enter the ist number: "))
    num1= int(input("Enter the ist number: "))
